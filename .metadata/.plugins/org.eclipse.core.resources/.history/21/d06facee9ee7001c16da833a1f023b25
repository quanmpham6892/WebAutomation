package excercise;

public class ReverseString {

	public static void main(String[] args) {
		// reverse a string
		String N = "tomorrow";
		
		char[] nChar = N.toCharArray();
		
		String result = "";
		
		for(int i=N.length()-1; i>=0; i--) {
			result = result + nChar[i];
		}
		
		System.out.println(result);
		

	}

}
