package conditions;

public class IfElseMultipleCondition {

	public static void main(String[] args) {
		
		int a = 10;
		int b = 20;
		int c = 5;
		
		//AND statement
		if(a<b && a<c) {
			System.out.println("Test");
		}else {
			System.out.println("false");
			}
		
		
		//OR condition
		if(a<b || a<c) {
			System.out.println("Test");
		}else {
			System.out.println("false");
			}
		
	}

}
