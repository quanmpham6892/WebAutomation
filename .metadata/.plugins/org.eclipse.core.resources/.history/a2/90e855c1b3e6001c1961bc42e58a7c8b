package class2;

public class LearningVariables {

	public static void main(String args) {
		
		//Variable - container(bucket) //dataType
		//Formula: dataType variableName = value;
		int a = 10;
		long drt4rtfbf = 2727327367267368L;
		String A = "Hello my name is Quanstah";
		char b = 'c';
		boolean d = true;
		float e = 5.38724987389273827832748932f;
		double ee = 5.793824983982923849287579252;
		System.out.println(a);
		System.out.println(A);
		System.out.println(a);

		System.out.println(a);

		System.out.println(a);

		System.out.println(a);

		
		
		
	}
}
